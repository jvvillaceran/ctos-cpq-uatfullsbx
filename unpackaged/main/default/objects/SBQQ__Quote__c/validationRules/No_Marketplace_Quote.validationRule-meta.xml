<?xml version="1.0" encoding="utf-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>No_Marketplace_Quote</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
  ISCHANGED(SBQQ__Type__c), 
  ISPICKVAL(SBQQ__Type__c, "Marketplace"),  
   $User.Marketplace_User__c = FALSE
)</errorConditionFormula>
    <errorMessage>You do not have permissions to create Marketplace Quotes. Please contact the System Administrator</errorMessage>
</ValidationRule>
