<?xml version="1.0" encoding="utf-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Markeplace_Product_only_on_MP_Quote</fullName>
    <active>true</active>
    <errorConditionFormula>AND(NOT(ISPICKVAL(SBQQ__Quote__r.SBQQ__Type__c,"Marketplace")),  SBQQ__Product__r.Name = "Marketplace")</errorConditionFormula>
    <errorMessage>This product cannot be used on this quote</errorMessage>
</ValidationRule>
